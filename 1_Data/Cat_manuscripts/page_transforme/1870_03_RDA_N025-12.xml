    <TextRegion type="paragraph" id="r_2_16" custom="readingOrder {index:16;}">
      <Coords points="376,1146 1756,1146 1756,1345 376,1345"/>
      <TextLine id="tl_23" primaryLanguage="English" custom="structure {type:default;} readingOrder {index:0;}">
        <Coords points="404,1147 1750,1147 1750,1189 404,1189"/>
        <Baseline points="404,1179 1750,1179"/>
        <Word id="w_w1aab1b3b2c31b1ab1">
          <Coords points="404,1147 1750,1147 1750,1189 404,1189"/>
          <TextEquiv>
            <Unicode>Curieuse épître. Il le remercie d'un article élogieux, qui lui a fait grand plaisir.</Unicode>
          </TextEquiv>
          <TextStyle fontFamily="Times New Roman" fontSize="8.0"/>
        </Word>
      </TextLine>
      <TextLine id="tl_24" primaryLanguage="English" custom="structure {type:default;} readingOrder {index:1;}">
        <Coords points="380,1187 1755,1187 1755,1227 380,1227"/>
        <Baseline points="380,1218 1755,1218"/>
        <Word id="w_w1aab1b3b2c31b3ab1">
          <Coords points="380,1187 1755,1187 1755,1227 380,1227"/>
          <TextEquiv>
            <Unicode>« Du moins, dit-il, autant que puisse me faire plaisir une preuve d’amitié, car je</Unicode>
          </TextEquiv>
          <TextStyle fontFamily="Times New Roman" fontSize="8.0"/>
        </Word>
      </TextLine>
      <TextLine id="tl_25" primaryLanguage="English" custom="structure {type:default;} readingOrder {index:2;}">
        <Coords points="377,1227 1752,1227 1752,1266 377,1266"/>
        <Baseline points="377,1257 1752,1257"/>
        <Word id="w_w1aab1b3b2c31b5ab1">
          <Coords points="377,1227 1752,1227 1752,1266 377,1266"/>
          <TextEquiv>
            <Unicode>deviens de plus en plus indifférent à tout. Les douleurs physiques m’exténuent,</Unicode>
          </TextEquiv>
          <TextStyle fontFamily="Times New Roman" fontSize="8.0"/>
        </Word>
      </TextLine>
      <TextLine id="tl_26" primaryLanguage="English" custom="structure {type:default;} readingOrder {index:3;}">
        <Coords points="378,1266 1752,1266 1752,1305 378,1305"/>
        <Baseline points="378,1296 1752,1296"/>
        <Word id="w_w1aab1b3b2c31b7ab1">
          <Coords points="378,1266 1752,1266 1752,1305 378,1305"/>
          <TextEquiv>
            <Unicode>l’isolement et l’ennui m’écrasent, et je n’aspire plus qu’à m'en aller vite en terre,</Unicode>
          </TextEquiv>
          <TextStyle fontFamily="Times New Roman" fontSize="8.0"/>
        </Word>
      </TextLine>
      <TextLine id="tl_27" primaryLanguage="English" custom="structure {type:default;} readingOrder {index:4;}">
        <Coords points="378,1305 1692,1305 1692,1344 378,1344"/>
        <Baseline points="378,1335 1692,1335"/>
        <Word id="w_w1aab1b3b2c31b9ab1">
          <Coords points="378,1305 1692,1305 1692,1344 378,1344"/>
          <TextEquiv>
            <Unicode>comme Marlborough, sans même être porté comme lui par quatre-z-ofticiers. »</Unicode>
          </TextEquiv>
          <TextStyle fontFamily="Times New Roman" fontSize="8.0"/>
        </Word>
      </TextLine>
    </TextRegion>
