      <TextLine id="tl_39" primaryLanguage="English" custom="structure {type:default;} readingOrder {index:0;}">
        <Coords points="802,2693 3006,2693 3006,2733 802,2733"/>
        <Baseline points="802,2723 815,2723 908,2725 1001,2725 1092,2732 1194,2724 1288,2723 1462,2723 1548,2723 1596,2723 1706,2723 1812,2723 1942,2722 1998,2722 2097,2722 2293,2724 2356,2723 2433,2724 2647,2724 2729,2724 2797,2725 2962,2725 3006,2725"/>
        <Word id="w_w2aab1c15b2b7b1ab1">
          <Coords points="802,2693 3006,2693 3006,2733 802,2733"/>
          <TextEquiv>
            <Unicode>« Mon cher ami, voilà trois semaines que je veux vous écrire et vous remercier de vos sentiments qui me touchent et</Unicode>
          </TextEquiv>
          <TextStyle fontFamily="Times New Roman" fontSize="7.0"/>
        </Word>
      </TextLine>
      <TextLine id="tl_40" primaryLanguage="English" custom="structure {type:default;} readingOrder {index:1;}">
        <Coords points="764,2735 3004,2735 3004,2775 764,2775"/>
        <Baseline points="764,2766 813,2767 993,2767 1047,2766 1236,2765 1337,2764 1511,2764 1589,2764 1635,2764 1715,2765 1790,2765 1847,2764 1982,2763 2083,2764 2138,2765 2226,2766 2357,2766 2430,2766 2560,2766 2610,2766 2729,2766 2820,2765 2875,2765 3004,2765"/>
        <Word id="w_w2aab1c15b2b7b3ab1">
          <Coords points="764,2735 3004,2735 3004,2775 764,2775"/>
          <TextEquiv>
            <Unicode>me pénètrent de gratitude. Mais imaginez que je n’ai pas pu encore jouir de mon congé. J’ai autant et même plus de travail</Unicode>
          </TextEquiv>
          <TextStyle fontFamily="Times New Roman" fontSize="7.0"/>
        </Word>
      </TextLine>
      <TextLine id="tl_41" primaryLanguage="English" custom="structure {type:default;} readingOrder {index:2;}">
        <Coords points="763,2777 3008,2777 3008,2817 763,2817"/>
